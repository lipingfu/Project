# CosmoPMC
setenv COSMOPMC $HOME/astro/ECOSSTAT/trunk
setenv ECOR $COSMOPMC/R


# my path
setenv RPATH $HOME/astro/svn/lensr-1.2/Scripts 
setenv PERL5LIB $HOME/astro/svn/lensr-1.2/Scripts
setenv  PYTHONPATH $HOME/astro/astro_share/python


set mypath = ($HOME/bin $HOME/astro/svn/python $HOME/astro/svn/R $HOME/astro/svn/C4algorithm/src $HOME/astro/svn/C++/src  $HOME/astro/svn/lensr-1.2/Scripts $HOME/astro/svn/lensr-1.2/src $HOME/astro/astro_share/bin $HOME/astro/athena/bin $HOME/astro/svn/perl $HOME/astro/Corr $HOME/astro/astro_share/covgauss  $HOME/astro/astro_share/covgauss/EBmixing  $HOME/Software/theli-1.2.1/bin/Linux_64 $HOME/astro/svn/perl/ $HOME/astro/astro_share/perl)  

set allpath = (/opt/intel/bin/  /usr/local/cfitsio)

set PMCpath = ($COSMOPMC/bin $COSMOPMC/exec  $COSMOPMC/mring/test  $COSMOPMC/camb)

set path = ($path $RPATH  $allpath $PMCpath $mypath)




 setenv LS_COLORS 'no=00:fi=00:di=01;34:ln=01;36:pi=40;33:so=01;35:bd=40;33;01:cd=40;33;01:or=01;05;37;41:mi=01;05;37;41:ex=01;32:*.cmd=01;35:*.exe=01;35:*.com=01;35:*.btm=01;35:*.bat=01;35:*.sh=01;35:*.csh=01;35:*.tar=01;31:*.tgz=01;31:*.arj=01;31:*.taz=01;31:*.lzh=01;31:*.zip=01;31:*.z=01;31:*.Z=01;31:*.gz=01;31:*.bz2=01;31:*.bz=01;31:*.tz=01;31:*.rpm=01;31:*.cpio=01;31:*.jpg=01;35:*.gif=01;35:*.bmp=01;35:*.xbm=01;35:*.xpm=01;35:*.png=01;35:*.tif=01;35'




setenv CPPFLAGS " -I/usr/local/include    -I/usr/local -I/usr/local/include/plplot/ -I/opt/intel/composerxe/mkl/include"
setenv LDFLAGS "-L/usr/lib64  -L/usr/local/lib -L/usr/local/cfitsio/lib/ -L/opt/intel/composerxe/mkl/lib/intel64/ -L/opt/intel/composer_xe_2013.0.079/compiler/lib/intel64 "
setenv LD_LIBRARY_PATH  /usr/local/cfitsio/lib/:/usr/lib64:/usr/local/lib


#theli
setenv PIPESOFT /usr/local/software/theli-1.6.1
setenv EXT Linux_64 

setenv PIPEBIN ${PIPESOFT}/bin
setenv PIPELIB ${PIPESOFT}/lib
setenv OBJSUB obj_${EXT}
setenv PIPECONF ${PIPESOFT}/conf
setenv PIPEDATA ${PIPESOFT}/data
set path=($path $PIPEBIN/${EXT} $PIPESOFT/scripts)

if ($?PERL5LIB) then
    setenv PERL5LIB ${PERL5LIB}:${PIPESOFT}/scripts/astrometrix
else
    setenv PERL5LIB ${PIPESOFT}/scripts/astrometrix
endif

