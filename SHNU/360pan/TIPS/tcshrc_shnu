#alias ftpmario "ftp anonymous@ftp.pd.astro.it"

alias ssh "ssh -X -Y"
alias emacs 'emacs -fn 9x15bold'
alias e 'emacs -fn 9x15bold'
alias enw 'emacs -nw -fn 9x15bold'
alias byorick  "yorick -batch"
alias ll 'ls -l'
alias lsdir "ls -l \!* |grep drw"
alias recent "ll -t \!* |head"
alias cpy "cp --reply y "
alias rm 'rm -f'
alias cp 'cp -f'
alias mv 'mv -f'
alias nice19  'nice +19'
alias gnu "gnuplot"
alias ffind 'find . -name '
alias svncom "svn --force-log commit -m"
alias topasc "topcat -f ascii"
alias topfit "topcat -f fits"
alias xterm "gnome-terminal &"

bindkey "^R"  i-search-back
bindkey -c "^O" "source ~/.tcshrc"

set complete=enhance
set autolist set color set colorcat
set nobeep
set correct no

complete kdvi "n/*/f:*.dvi/"
complete kill   c/-/S/ c/%/j/ 'n/*/`ps xh |cut1`/'
complete gv 'p/*/f:{0*,*.{ps,ps.gz,pstex,eps,epsi,psc}}/'
complete xpdf "n/*/f:*.pdf/"
complete acroread "n/*/f:*.pdf/"

# different color
set prompt = '%{\033[35m%}keylab% %c2 %b%{\033[0m%}] '
set OS = `/bin/sh -c '(/bin/uname||/usr/5bin/uname) 2>/dev/null'`

